{
  "name": "esazykin/laravel-clickhouse",
  "type": "library",
  "license": "MIT",
  "keywords": [
    "laravel",
    "clickhouse",
    "eloquent"
  ],
  "require": {
    "php": ">=7.1",
    "laravel/framework": "5.5.*|5.6.*|5.7.*|5.8.*|6.0.*|6.1.*|6.2.*",
    "esazykin/clickhouse-builder": "^2.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.5",
    "mockery/mockery": "^1.0",
    "fzaninotto/faker": "^1.7"
  },
  "autoload": {
    "psr-4": {
      "Esazykin\\LaravelClickHouse\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Esazykin\\LaravelClickHouse\\Tests\\": "tests/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit --stop-on-failure tests/"
  }
}
