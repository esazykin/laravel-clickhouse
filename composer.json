{
    "name": "michaelwise/laravel-clickhouse",
    "type": "library",
    "license": "MIT",
    "keywords": [
        "laravel",
        "clickhouse",
        "eloquent"
    ],
    "require": {
        "php": "^7.2|^8.0",
        "laravel/framework": "^9.0",
        "the-tinderbox/clickhouse-builder": "^5.3"
    },
    "require-dev": {
        "infection/infection": "~0.15",
        "phpunit/phpunit": "^8.0|^9.0",
        "mockery/mockery": "^1.3",
        "fakerphp/faker": "^1.9",
        "bavix/clickhouse-php-client": "^3.0",
        "bavix/clickhouse-builder": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Esazykin\\LaravelClickHouse\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Esazykin\\LaravelClickHouse\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --stop-on-failure tests/"
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
